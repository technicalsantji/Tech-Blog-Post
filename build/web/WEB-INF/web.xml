<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Register</servlet-name>
        <servlet-class>sant.servlet.Register</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>sant.servlet.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Logout</servlet-name>
        <servlet-class>sant.servlet.Logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Edit_Profile</servlet-name>
        <servlet-class>sant.servlet.Edit_Profile</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PostDetails</servlet-name>
        <servlet-class>sant.servlet.PostDetails</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Likes</servlet-name>
        <servlet-class>sant.servlet.Likes</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminLogin</servlet-name>
        <servlet-class>sant.servlet.AdminLogin</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Register</servlet-name>
        <url-pattern>/Register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Logout</servlet-name>
        <url-pattern>/Logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Edit_Profile</servlet-name>
        <url-pattern>/Edit_Profile</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PostDetails</servlet-name>
        <url-pattern>/PostDetails</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Likes</servlet-name>
        <url-pattern>/Likes</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminLogin</servlet-name>
        <url-pattern>/AdminLogin</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
